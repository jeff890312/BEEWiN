</model>

@if (TempData["LoginFail"] == "on")
{
    <script>
        document.getElementById("login-fail").style.display = "";
        document.getElementById("modal-login").classList.add('modal-popup');
        document.getElementById("login-email").value = "@TempData["LoginEmail"]";
    </script>
}
@if (TempData["RegisterFail"] == "on")
{
