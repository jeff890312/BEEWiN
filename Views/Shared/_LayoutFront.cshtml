<model>
    <model>
        <div>
            <script>
                function replace(hide, hideq, show, showq) {
                    document.getElementById(hide).style.display = "none";
                    document.getElementById(hideq).style.display = "none";
                    document.getElementById(show).style.display = "";
                    document.getElementById(showq).style.display = "";
                }
                function Agree(c) {
                    if (c.checked) {
                        document.getElementById("topay").classList.remove("disabled");
                        document.getElementById('topay').style.backgroundColor = "#cab380";
                        document.getElementById('topay').style.borderColor = "#cab380";
                    } else {
                        document.getElementById("topay").classList.add('disabled');
                        document.getElementById('topay').style.backgroundColor = "darkgrey";
                        document.getElementById('topay').style.borderColor = "darkgrey";
                    }
                }
                function copyEvent(id) {
                    var str = document.getElementById(id);
                    window.getSelection().selectAllChildren(str);
                    document.execCommand("Copy");
                    Swal.fire({
                        icon: 'success',
                        title: '複製成功',
                        text: '',
                        showConfirmButton: false,
                        timer: 1000,
                        width: 300,
                    });
                }
            </script>
            <style>
                .copy1 {
                    background-image: url('../../Template/Front/image/copy.svg');
                    width: 24px;
                    height: 24px;
                    border: none;
