    <!-- 登入 -->
    <div class="modal fade" id="modal-login" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-sm">
            <div class="modal-content text-center">
                <div class="modal-body">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    <h3 class="mb-4">會員登入</h3>

                    <div class="alert" style="background-color: #FEF6D8;">
                        <span class="sidebar-icon text-success"><span class="fas fa-lock"></span></span>
                        <small style="color: #1E2329; font-weight: 500;">資料傳輸受SSL(Secure Sockets Layer)加密保護</small>
                    </div>

                    <div id="login-fail" class="alert alert-danger" style="display:none">
                        <span class="sidebar-icon"><span class="fas fa-check"></span></span>
                        <small><strong> 登入失敗!</strong>請重新確認帳號及密碼。</small>
                    </div>

                    <form class="text-start mb-3" action="@Url.Action("Login", "Account")" method="post">
                        <div class="form-group form-label-group mb-3">
                            <input type="email" class="form-control" id="login-email" name="email" placeholder="信箱帳號" required>
                            <label for="email"><i class="fas fa-envelope"></i> 帳號</label>
                        </div>
                        <div class="form-group form-label-group mb-3">
                            <input type="password" class="form-control" name="password" placeholder="密碼" required>
                            <label for="password"><i class="fas fa-key"></i> 密碼</label>
                        </div>
                        <div>
                            <button style="width:100%" type="submit" class="btn btn-primary rounded btn-login w-100">Log In</button>
                        </div>
                    </form>

                    <div class="divider-icon my-4">or</div>
                    <p class="mb-1">
                        <a href="@Url.Action("forgotpassword","account")" class="hover">忘記密碼?</a>
                    </p>
                    <p class="mb-0">
                        還不是會員嗎 ?
                        <a href="#" class="hover" data-bs-target="#modal-register" data-bs-toggle="modal" data-bs-dismiss="modal">點擊註冊</a>
                    </p>
                </div>
                <!--/.modal-content -->
            </div>
            <!--/.modal-body -->
        </div>
        <!--/.modal-dialog -->
    </div>
    <!-- 註冊 -->
    <div class="modal fade" id="modal-register" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-sm">
            <div class="modal-content text-center">
                <div class="modal-body">
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    <h3 class="mb-4">會員註冊</h3>

                    <div class="alert" style="background-color: #FEF6D8;">
                        <span class="sidebar-icon text-success"><span class="fas fa-lock"></span></span>
                        <small style="color: #1E2329; font-weight: 500;">資料傳輸受SSL(Secure Sockets Layer)加密保護</small>
                    </div>

                    <div id="register-fail" class="alert alert-danger" style="display:none">
                        <span class="sidebar-icon"><span class="fas fa-check"></span></span>
                        <small><strong> 註冊失敗!</strong>該帳號已被註冊。</small>
                    </div>

                    <form class="text-start mb-3" action="@Url.Action("Register", "Account")" method="post">
                        <div class="form-group form-label-group mb-3">
                            <input type="email" class="form-control" name="email" placeholder="信箱帳號" required>
                            <label for="email"><i class="fas fa-envelope"></i> Email帳號</label>
                        </div>
                        <div class="form-group form-label-group mb-3">
                            <input type="password" class="form-control" name="Password" placeholder="密碼" pattern="(?=.*\d)(?=.*[a-zA-Z]).{8,}" title="請輸入只少8位英數字" required>
                            <label for="password"><i class="fas fa-key"></i> 請輸入只少8位英數字密碼</label>
                        </div>
                        <div class="form-group form-label-group mb-3">
                            <input type="text" class="form-control" name="Name" placeholder="暱稱" required>
                            <label for="Name"><i class="fas fa-signature"></i> 會員暱稱</label>
                        </div>
                        <div class="form-group form-label-group mb-3">
                            <input type="text" class="form-control" name="Country" placeholder="國家" required>
                            <label for="text"><i class="fas fa-globe-americas"></i> 國家</label>
                        </div>
                        <div class="form-group form-label-group mb-3">
                            <input type="tel" class="form-control" name="Phone" placeholder="手機號碼" required>
                            <label for="phone"><i class="fas fa-mobile-alt"></i> 手機</label>
                        </div>
                        <div>
                            <button style="width:100%" type="submit" class="btn btn-primary rounded btn-login w-100">註冊會員</button>
                        </div>
                    </form>

                    <div class="divider-icon my-4">or</div>
                    <p class="mb-1">
                        <a href="@Url.Action("forgotpassword","account")" class="hover">忘記密碼?</a>
                    </p>
                    <p class="mb-0">
                        已經是會員嗎 ?
                        <a href="#" class="hover" data-bs-target="#modal-login" data-bs-toggle="modal" data-bs-dismiss="modal">點我登入</a>
                    </p>
                </div>
                <!--/.modal-content -->
            </div>
            <!--/.modal-body -->
        </div>
        <!--/.modal-dialog -->
    </div>

